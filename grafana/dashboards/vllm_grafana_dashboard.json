      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (rate(vllm:e2e_request_latency_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p50",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(vllm:e2e_request_latency_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "B",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 8,
      "title": "Phase times (p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
