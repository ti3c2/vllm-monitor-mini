{
  "uid": "vllm-55f8eb41",
  "title": "vLLM Metrics (Prometheus)",
  "schemaVersion": 38,
  "version": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timezone": "browser",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Requests running",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(vllm:num_requests_running{job=\"$job\", instance=~\"$instance\", model_name=~\"$model\"})",
          "refId": "A",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "last"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto",
        "percentChangeColorMode": "standard"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Requests waiting",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(vllm:num_requests_waiting{job=\"$job\", instance=~\"$instance\", model_name=~\"$model\"})",
          "refId": "A",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "last"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto",
        "percentChangeColorMode": "standard"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "Request rate (RPS)",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "req/s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{handler=\"/v1/chat/completions\",method=\"POST\",status=~\"2..\",job=\"$job\",instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "2xx RPS",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        },
        {
          "expr": "sum(rate(http_requests_total{handler=\"/v1/chat/completions\",method=\"POST\",status!~\"2..\",job=\"$job\",instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "non-2xx RPS",
          "refId": "B",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 4,
      "title": "KV cache usage (%)",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "100 * avg(vllm:kv_cache_usage_perc{job=\"$job\", instance=~\"$instance\", model_name=~\"$model\"})",
          "legendFormat": "KV usage %",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 5,
      "title": "Prefix cache hit rate",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "(sum(rate(vllm:prefix_cache_hits_total{job=\"$job\", instance=~\"$instance\", model_name=~\"$model\"}[$__rate_interval])) / sum(rate(vllm:prefix_cache_queries_total{job=\"$job\", instance=~\"$instance\", model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "hit rate",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 6,
      "title": "TTFT (p50/p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (rate(vllm:time_to_first_token_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p50",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(vllm:time_to_first_token_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "B",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 7,
      "title": "E2E latency (p50/p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
