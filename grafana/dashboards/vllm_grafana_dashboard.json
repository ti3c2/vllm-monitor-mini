      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(vllm:request_prefill_time_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "prefill p95",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(vllm:request_decode_time_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "decode p95",
          "refId": "B",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(vllm:request_inference_time_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "inference p95",
          "refId": "C",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 9,
      "title": "Queue time (p50/p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (rate(vllm:request_queue_time_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p50",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(vllm:request_queue_time_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "B",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 10,
      "title": "Generation tokens per second",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" }, "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(vllm:generation_tokens_total{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval]))",
          "legendFormat": "generation tok/s",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": { "type": "prometheus", "uid": "${datasource}" }
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "id": 17,
      "title": "Prompt tokens per second",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 9 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" }, "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(vllm:prompt_tokens_total{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval]))",
          "legendFormat": "prompt tok/s",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": { "type": "prometheus", "uid": "${datasource}" }
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    },
    {
      "id": 11,
      "title": "CPU usage (cores)",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(process_cpu_seconds_total{job=\"$job\",instance=~\"$instance\"}[$__rate_interval]))",
          "legendFormat": "CPU cores",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 12,
      "title": "Process RSS",
      "type": "timeseries",
      "gridPos": {
        "h": 9,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line"
          },
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "avg(process_resident_memory_bytes{job=\"$job\",instance=~\"$instance\"})",
          "legendFormat": "RSS",
          "refId": "A",
          "instant": false,
          "range": true,
          "editorMode": "code",
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          }
        }
      ],
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      }
    },
    {
      "id": 13,
      "type": "table",
      "title": "Success breakdown (by finished_reason)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum by (finished_reason) (increase(vllm:request_success_total{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__range]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "showHeader": true,
        "footer": {
          "show": false
        }
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "id": 14,
      "type": "stat",
      "title": "Avg prompt throughput (tok/s, 5m)",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 85 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "avg_over_time(sum(rate(vllm:prompt_tokens_total{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval]))[5m:])",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "id": 15,
      "type": "stat",
      "title": "Avg generation throughput (tok/s, 5m)",
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 85 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "avg_over_time(sum(rate(vllm:generation_tokens_total{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval]))[5m:])",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "textMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] }
    },
    {
      "id": 16,
      "title": "Generation speed (tok/s p50/p95)",
      "type": "timeseries",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 90 },
      "fieldConfig": { "defaults": { "custom": { "drawStyle": "line" }, "unit": "ops" }, "overrides": [] },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "1 / histogram_quantile(0.5, sum by (le) (rate(vllm:time_per_output_token_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p50 tok/s",
          "refId": "A"
        },
        {
          "expr": "1 / histogram_quantile(0.95, sum by (le) (rate(vllm:time_per_output_token_seconds_bucket{job=\"$job\",instance=~\"$instance\",model_name=~\"$model\"}[$__rate_interval])))",
          "legendFormat": "p95 tok/s",
          "refId": "B"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${datasource}" }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "current": {
          "selected": true
        }
      },
      {
        "name": "job",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "refresh": 1,
        "query": "label_values(vllm:num_requests_running, job)",
        "current": {
          "text": "vllm",
          "value": "vllm",
          "selected": true
        },
        "includeAll": true,
        "label": "job"
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "refresh": 1,
        "query": "label_values(vllm:num_requests_running{job=\"$job\"}, instance)",
        "includeAll": true,
        "label": "instance"
      },
      {
        "name": "model",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "refresh": 1,
        "query": "label_values(vllm:num_requests_running{job=\"$job\",instance=~\"$instance\"}, model_name)",
        "includeAll": true,
        "label": "model_name"
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}